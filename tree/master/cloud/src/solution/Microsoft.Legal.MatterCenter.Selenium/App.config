    <add key="UserIDSelector" value="document.getElementById('cred_userid_inputtext').value='matteradmin@msmatter.onmicrosoft.com'" />
    <!--Update the value with Username-->
    <add key="UserPasswordSelector" value="document.getElementById('cred_password_inputtext').setAttribute('value','P@$$w0rd01')" />
