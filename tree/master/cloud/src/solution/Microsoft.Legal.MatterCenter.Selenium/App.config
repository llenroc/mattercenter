    <add key="UserIDTextBox" value="cred_userid_inputtext" />
    <add key="UserPasswordTextBox" value="cred_password_inputtext" />
    <add key="SignInButton" value="cred_sign_in_button" />
    <add key="KeepSignInButtonCheckBox" value="document.getElementById('cred_keep_me_signed_in_checkbox').checked = true;" />
    
  </appSettings>
